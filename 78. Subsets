class Solution {
    public void findSubsets(int[] nums,int idx,List<List<Integer>> ans,List<Integer> ds){
        if(idx==nums.length){
            ans.add(new ArrayList<>(ds));
            return;
        }
        ds.add(nums[idx]);
        findSubsets(nums,idx+1,ans,ds);
        ds.remove(ds.size()-1);

        findSubsets(nums,idx+1,ans,ds);
    }
    public List<List<Integer>> subsets(int[] nums) {
        List<List<Integer>> ans=new ArrayList<>();
        findSubsets(nums,0,ans, new ArrayList<>());
        return ans;
    }
}
